def доповнити_список_парними_елементами(список):
  парні_елементи = []
  for індекс, елемент in enumerate(список):
      if індекс % 2 == 0:
          парні_елементи.append(елемент)

  список.extend(парні_елементи)
  print(список)

введений_список = input("Введіть елементи списку, розділені пробілами: ")
список = введений_список.split()

доповнити_список_парними_елементами(список)
