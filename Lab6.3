  x = set(range(8, 23))

  def is_prime(num):
      if num <= 1:
          return False
      for i in range(2, int(num**0.5) + 1):
          if num % i == 0:
              return False
      return True

  y = set(num for num in x if is_prime(num))

  z = set()

  try:
      z = x.union(y)
  except AttributeError:
